@*View to edit user*@
@model DealSe.Areas.Admin.FormModels.UserFormModel
@{
    ViewData["Title"] = "Edit User";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<ol class="breadcrumb">
    <li>
        <a href="@Url.Action("Index", "Home" , new {Area="Admin" })"><i class="entypo-home"></i>Home</a>
    </li>
    <li><a href="@Url.Action("Index", "User" , new { Area="Admin" })"><i class="entypo-users"></i>Users</a></li>
    <li class="active">
        <strong>View User</strong>
    </li>
</ol>
<h2>View User</h2>
<br />
@if (ViewBag.Error != null)
{
    <div class="row">
        <div class="col-md-12">
            <div class="alert alert-danger"><strong>Oops!</strong> @ViewBag.Error</div>
        </div>
    </div>
}

@Html.HiddenFor(Model => Model.InvalidLoginAttemptCount)
@Html.HiddenFor(Model => Model.LastInvalidLoginAttemptDate)
@await Html.PartialAsync("_UserInfo", Model)
@await Html.PartialAsync("_CommonDateCssJSView")